package co.blocke.scalajack
package json
package run

sealed trait Node:
  val id: String

final case class SegmentNode(
    id: String
) extends Node

final case class LogicalNode(
    id: String,
    blah: Int,
    children: Vector[Node]
) extends Node

opaque type THING = String
case class Foo[T](ln: T)

